{
    "name": "pr-reviewer",
    "displayName": "PR Reviewer",
    "description": "Review pull requests using VS Code's Language Model",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.85.0"
    },
    "activationEvents": ["onCommand:pr-reviewer.review"],
    "main": "./extension.js",
    "contributes": {
        "commands": [{
            "command": "pr-reviewer.review",
            "title": "Review Pull Request"
        }]
    },
    "scripts": {
        "lint": "eslint .",
        "test": "node ./test/runTest.js"
    },
    "devDependencies": {
        "@types/vscode": "^1.85.0",
        "eslint": "^8.56.0"
    }
}
